                                    </xsd:sequence> 
                                </xsd:complexType> 
                            </xsd:element> 
                        </xsd:sequence> 
                    </xsd:complexType> 
                </xsd:element> 
                <xsd:element maxOccurs="1" minOccurs="0" name="SearchCriteria" nillable="false" type="text"/>  
            </xsd:all> 
        </xsd:complexType>  
        <xsd:unique name="BrowseItem"> 
            <xsd:selector xpath="."/>  
            <xsd:field xpath="CriteriaName"/> 
        </xsd:unique> 
    </xsd:element> 
     <xsd:element name="HierarchySearchItem">
  <xsd:complexType>
<xsd:sequence>
